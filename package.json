{
  "name": "compile-tools",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "g:test": "cd $INIT_CWD && jest --runInBand --detectOpenHandles --forceExit",
    "g:integration-test": "./runIntegrationTest.sh",
    "prelint": "yarn constraints",
    "lint": "yarn prelint && yarn eslint . --cache && yarn postlint",
    "postlint": "yarn tsc",
    "pretest": "yarn lint",
    "test": "yarn pretest && yarn jest --runInBand --detectOpenHandles --forceExit",
    "clean": "yarn tsc --build --clean; rm -rf tsconfig.build.tsbuildinfo tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@lifeomic/eslint-config-standards": "^2.1.1",
    "@lifeomic/jest-config": "^1.1.2",
    "@lifeomic/test-tool-utils": "^2.0.1",
    "@lifeomic/typescript-config": "^1.0.3",
    "@swc/core": "^1.2.161",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.18.12",
    "@types/semantic-release": "^17.2.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^8.12.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jsdoc": "^38.1.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "semantic-release": "^17.4.7",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "ulid": "^2.3.0",
    "verdaccio": "^5.8.0"
  },
  "engines": {
    "node": ">=14.14.0"
  },
  "repository": {
    "directory": ".",
    "type": "git",
    "url": "https://github.com/lifeomic/compile-tools.git"
  }
}
