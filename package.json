{
  "name": "compile-tools",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "g:test": "cd $INIT_CWD && jest",
    "prelint": "yarn constraints",
    "lint": "yarn prelint && yarn eslint . --cache && yarn postlint",
    "postlint": "yarn tsc",
    "pretest": "yarn lint",
    "test": "yarn pretest && yarn jest",
    "clean": "yarn tsc --build --clean; rm -rf tsconfig.build.tsbuildinfo tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@lifeomic/eslint-config-standards": "^2.1.1",
    "@lifeomic/jest-config": "^1.1.2",
    "@lifeomic/typescript-config": "^1.0.3",
    "@swc/core": "^1.2.147",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.20.46",
    "@types/semantic-release": "^17.2.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^8.10.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsdoc": "^37.9.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "semantic-release": "^17.4.7",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=12.12.6"
  },
  "repository": {
    "directory": ".",
    "type": "git",
    "url": "https://github.com/lifeomic/compile-tools.git"
  }
}
